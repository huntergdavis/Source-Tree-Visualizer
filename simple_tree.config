<xml>
	<simple_tree_options>
		<image_width>800</image_width>
		<image_height>800</image_height>
		<start_width>6000</start_width>
		<start_height>6000</start_height>	
		<output_file_number>10000</output_file_number>
		<!-- background_image>/home/programmer/Projects/source_tree_vis/resources/backgrounds/venice_beach.jpg</background_image -->
		<many_jpgs>0</many_jpgs>
		<jpg_step>1</jpg_step>
		<jpg_stop>10000</jpg_stop>
		<jpg_start>1</jpg_start>
		<rev_jpgs>1</rev_jpgs>
		<rev_step>1</rev_step>
		<rev_stop>10000</rev_stop>
		<rev_start>1</rev_start>	
		<debug>default</debug>
		<file_name>ourlocaltree.jpg</file_name>
		<repo_type>git</repo_type> 
		<repo_string>~/Projects/source_tree_vis</repo_string>
		<input_filter>.cpp,Makefile<color>red</color><alt_color>blue</alt_color><type>source files</type></input_filter>
		<input_filter>.h<color>green</color><sub_color>brown</sub_color><type>header files</type></input_filter>
		<input_filter>.jpg,.gif,.bmp<color>purple</color><hover_color>orange</hover_color><type>images</type></input_filter>
		<input_ignore_filter></input_ignore_filter>
		<draw_filtered_leaves>1</draw_filtered_leaves>
	</simple_tree_options>
</xml>
